prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: baryonyx
Description: An integer or binary linear programming solver
Version: @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@
URL: http://www.vle-project.org
Requires:
Cflags: -I${includedir}/baryonyx-@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@ -DBARYONYX_DLL
Libs: -L${libdir} -lbaryonyx-@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@

